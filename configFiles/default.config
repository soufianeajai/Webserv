
server

    host: 127.0.0.1
    port: 8081 4000

    location /
        methods: POST GET DELETE
        root: /tmp/www/dir
        autoindex: on
        default_file: index.html
        cgi_extension: php py
        redirect: new-path 301


    server_names: example.com example1.com example2.com
    server_root: /
    error_page 404 /errors/404.html
    error_page 405 /errors/405.html
    error_page 406 /errors/406.html
    error_page 407 /errors/407.html

    location /script
        methods: POST GET
        cgi_extension: php py bash

    location /uploads
        methods: POST DELETE
        root: /tmp/www/dir
        autoindex: off



