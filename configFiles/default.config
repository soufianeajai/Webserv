
server

    host: 127.0.0.1 
    port: 8080 9000

    server_names: example.com example1.com example2.com
    server_root: /
    error_page: 404 /www/html/errorPages/404.html
    error_page: 405 /www/html/errorPages/405.html
    error_page: 403 /www/html/errorPages/406.html
    error_page: 505 /www/html/errorPages/407.html

    location: /uploads/
        methods: POST DELETE GET
        root: /www/html/uploads
        autoindex: off



    location: /script/
        root: /www/html/cgi/
        default_file: default.php
        methods: POST GET
        cgi_extension: php py
