server
	host: 0.0.0.0
	port: 8080 8081 8082 8083
	server_names: example.com example1.com example2.com
	error_page 404 /errors/404.html
	error_page 405 /errors/405.html
	error_page 406 /errors/405.html
	error_page 407 /errors/405.html
	client_body_size: 100M
	redirect: old-path new-path 301
	server_root: /
	location 
		methods: POST GET DELETE
		root: /tmp/www/dir
		autoindex: on
		default_file: index.html
		cgi_extension: php py



	location /script
		methods: POST GET 
		cgi_extension: php py bash

	location /uploads
		methods: POST DELETE
		root: /tmp/www/dir
		autoindex: off

